{
  "expo": {
    "name": "QRypt",
    "slug": "qrypt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "qrypt",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F9FAFB",
        "foregroundImage": "./assets/images/android-adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kauavns.qrypt"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#F9FAFB"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permita $(PRODUCT_NAME) a acessar a sua câmera para escanear códigos QR."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "35d9ae2b-3dc2-4dda-b6d7-7f1c88723bdb"
      }
    },
    "owner": "kauavns"
  }
}
